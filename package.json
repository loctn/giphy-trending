{
	"name": "giphy-trending",
	"version": "0.0.1",
	"private": true,
	"description": "Giphy trending gifs",
	"main": "./server",
	"scripts": {
		"build": "webpack",
		"build:watch": "npm run build -- -w",
		"clean": "rm -r server/public/*",
		"dev": "NODE_PATH=./server nodemon --debug=8081 --inspect ./server",
		"precommit": "lint-staged",
		"start": "NODE_PATH=./server node ./server"
