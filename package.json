		"sass-loader": "6.0.6",
		"style-loader": "0.19.1",
		"url-loader": "0.6.2",
		"webpack": "3.10.0",
		"webpack-clean-obsolete-chunks": "0.3.0"
	},
	"dependencies": {
		"axios": "0.17.1",
		"babel-polyfill": "6.26.0",
		"babel-preset-es2015-ie": "6.7.0",
		"classnames": "2.2.5",
		"express": "4.16.2",
		"morgan": "1.9.0",
		"prop-types": "15.6.0",
		"react": "16.2.0",
		"react-css-modules": "4.7.1",
		"react-dom": "16.2.0",
		"react-redux": "5.0.6",
		"react-router-dom": "4.2.2",
		"redux": "3.7.2"
	},
	"engines": {
		"node": "8.9.3"
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"prettier --write",
			"git add"
		]
	},
	"prettier": {
		"singleQuote": true,
		"useTabs": true,
		"semi": false,
		"trailingComma": "es5"
	}
